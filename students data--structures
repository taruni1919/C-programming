#include<stdio.h>
struct sist
{
    char name;
    int regno;
   double mark;
}
s[3];
int main()
{
    struct sist s[3];
    for(int i=0;i<5;i++)
    {
    printf("\nenter the name %d",i+1);
    scanf("%s",&s[i].name);
    printf("\nenter thr regno %d",i+1);
    scanf("%d",&s[i].regno);
    printf("\nenter the mark  %d",i+1);
    scanf("%.11f",&s[i].mark);
    }
    for(int i=0;i<3;i++)
    {
        printf("%s",s[i].name);
        printf("%d",s[i].regno);
        printf("%11f",s[i].mark);
    }
    return 0;
}
