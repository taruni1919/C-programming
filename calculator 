#include <stdio.h>

void main() {

int a, b, result;

char op;

printf("Enter the values: "); scanf("%d %d", &a, &b);

printf("Enter the symbol: ");

scanf("%c", &op);

switch(op) {

case '+':

result = a + b;

break;

case '-':

result = a b;

break;

case '*':

result = a * b;

break;

case '/':

if (b!= 0) {

result = a/b;

} else {

printf("Error: Division by

zero\n");

return 1;

}

break;

default:

printf("Invalid Operator\n");

return 1;

}
printf("Result: %d\n", result);